<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>${project.parent.artifactId}</id>
	<name>${project.parent.name}</name>
	<version>${project.parent.version}</version>
	<package>${project.parent.groupId}.${project.parent.artifactId}</package>
	<author>OpenHMIS</author>
	<description>
		${project.parent.description}
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/${project.parent.artifactId}/update.rdf</updateURL>
	<require_version>${openMRSVersion}</require_version>
	<!-- / Module Properties -->

	<aware_of_modules>
		<aware_of_module version="3.3.1">org.openmrs.module.uiframework</aware_of_module>
		<aware_of_module version="2.3">org.openmrs.module.appframework</aware_of_module>
		<aware_of_module version="2.2">org.openmrs.module.providermanagement</aware_of_module>
		<aware_of_module version="1.7">org.openmrs.module.uicommons</aware_of_module>
	</aware_of_modules>

	<require_modules>
		<require_module version="2.6">org.openmrs.module.htmlformentry</require_module>
		<require_module version="1.5">org.openmrs.module.htmlformentryui</require_module>
		<require_module version="1.2">org.openmrs.module.visitdocumentsui</require_module>
		<require_module version="2.9">org.openmrs.module.webservices.rest</require_module>
		<require_module version="3.0">org.openmrs.module.openhmis.commons</require_module>
		<require_module version="1.11.1">org.openmrs.module.emrapi</require_module>
		<require_module version="2.3">org.openmrs.module.referenceapplication</require_module>
		<require_module version="1.4">org.openmrs.module.appui</require_module>
		<require_module version="1.7">org.openmrs.module.coreapps</require_module>
	</require_modules>

	<!-- Module Activator -->
	<activator>org.openmrs.module.customrestservices.CustomRESTModuleActivator</activator>


	<!-- Maps hibernate file's, if present -->

	<!-- Internationalization -->
	<!-- All message codes should start with ${project.parent.artifactId}. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

	<!-- Privileges -->
	<!-- App privileges for OpenMRS 2.x -->

	<privilege>
		<name>App: View Custom REST App</name>
		<description>Able to access the Custom REST module</description>
	</privilege>
</module>

